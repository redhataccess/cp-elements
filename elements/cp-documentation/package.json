{
  "name": "@cpelements/cp-documentation",
  "description": "Documentation element for PatternFly Elements",
  "version": "0.1.7",
  "pfelement": {
    "className": "PfeDocumentation",
    "elementName": "cp-documentation",
    "files": {
      "styles": "./src/cp-documentation.scss",
      "template": "./src/cp-documentation.html",
      "js": "./src/cp-documentation.js"
    },
    "assets": []
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "github:patternfly/patternfly-elements",
    "directory": "elements/cp-documentation"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cp-documentation.umd.js",
  "module": "./dist/cp-documentation.js",
  "unpkg": "./dist/cp-documentation.umd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "../../node_modules/.bin/gulp dev",
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json elements/cp-documentation/test/"
  },
  "contributors": [
    {
      "name": "wesruv"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@patternfly/pfelement": "1.0.0-prerelease.45"
  },
  "devDependencies": {
    "@patternfly/pfe-sass": "1.0.0-prerelease.45",
    "gh-pages": "^2.2.0",
    "pantheon-front-end": "^0.1.2",
    "pinkie-promise": "^2.0.1"
  }
}
